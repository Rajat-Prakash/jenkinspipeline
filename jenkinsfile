pipeline{
agent any
stages {

      stage('Publish SNS') {
         steps {
            echo 'Publishing SNS message to AWS'
            withAWS(credentials:'oxnbxE79jE7E63FZ8YHQAEx9lwOA85picSRdOeAK') {
                snsPublish(
                    topicArn:'arn:aws:sns:us-east-1:572411739171:mailNotification',
                    subject:"${params.ACTION} ${params.AWS_ENVIRONMENT}",
                    message: getMessage(mapEnvironmentToAWS[params.AWS_ENVIRONMENT], params.ACTION )
                    )
            }
         }
      }
   }
   }

arn:aws:iam::572411739171:user/rajat.prakash@publicissapient.com
arn:aws:sns:us-east-1:572411739171:mailNotification


